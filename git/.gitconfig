[user]
	name = Pranav RK
	email = pranav.r@rently.com
[core]
	editor = vim
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	ui = true
	autocrlf = input
[merge]
	tool = vimdiff
[help]
	autocorrect = 20
[alias]
	st = status
	ci = commit
  cm = commit -m
  ca = commit --amend
  br = branch
	co = checkout
	ssb = status -sb
	d = diff
	dt = difftool
[diff]
	tool = vimdiff
[difftool]
	prompt = true
[pull]
	rebase = false
[fetch]
	prune = true
[includeIf "gitdir:~/personal/"]
    path = ~/personal/.gitconfig
[init]
	defaultBranch = main
